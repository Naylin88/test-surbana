@model IEnumerable<ConsumeAPI.Models.SampleData>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h3 class="display-4">Consume Web API</h3>
    <p></p>

    <table class="table">
        <tr>
            <th>
                Device Name
            </th>
            <th>
                Occapancy
            </th>
            <th>
                Temperature
            </th>
            <th>
                Humidity
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DeviceName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Data.Occupancy)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Data.Temperature)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Data.Humidity)
                </td>
            </tr>
        }

    </table>
    <h4 class="text-success">@ViewBag.Message</h4>
</div>
